{
  "expo": {
    "name": "Bharat Stock",
    "slug": "bharatStock",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.svg",
    "scheme": "bharatStock",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icon.svg",
        "backgroundImage": "./assets/images/icon.svg",
        "monochromeImage": "./assets/images/icon.svg"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/icon.svg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.svg",
          "resizeMode": "contain",
          "imageWidth": 160,
          "backgroundColor": "#FFFFFF",
          "border": 0,
          "dark": {
            "image": "./assets/images/icon.svg",
            "resizeMode": "contain",
            "imageWidth": 160,
            "backgroundColor": "#020617"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2a1ef606-e726-4c50-9d00-7bebab722e89"
      }
    }
  }
}
